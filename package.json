{
  "name": "@spences10/marvel-names",
  "version": "1.3.0",
  "description": "Get random Marvel names",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "precommit": "yarn test",
    "prepush": "yarn test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spences10/marvel-names.git"
  },
  "keywords": [
    "random",
    "marvel",
    "names"
  ],
  "author": "Scott Spence <spences10apps@gmail.com> (https://spences10.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/marvel-names/issues"
  },
  "homepage": "https://github.com/spences10/marvel-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.0",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "semantic-release-cli": "^3.0.3"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
