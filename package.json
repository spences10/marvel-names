{
  "name": "@spences10/marvel-names",
  "version": "1.3.0",
  "description": "Get random Marvel names",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "precommit": "yarn test",
    "prepush": "yarn test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spences10/marvel-names.git"
  },
  "keywords": [
    "random",
    "marvel",
    "names"
  ],
  "author": "Scott Spence <spences10apps@gmail.com> (https://scottspence.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spences10/marvel-names/issues"
  },
  "homepage": "https://github.com/spences10/marvel-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "0.14.3",
    "jest": "^23.4.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.5",
    "semantic-release-cli": "4.0.6"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
